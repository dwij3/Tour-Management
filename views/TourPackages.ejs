<div class="container">
    <h1>Tour Packages</h1>
    <ul class="responsive-table">
      <li class="table-header">
        <!-- <div class="col col-1">Id</div> -->
        <div class="col col-2">Source</div>
        <div class="col col-3">Destination</div>
        <div class="col col-4">Duration</div>
        <div class="col col-5">Price</div>
        <div class="col col-6">Rating</div>
      </li>
      <% data.forEach(package => { %>
        <li class="table-row">
          <!-- <div class="col col-1" data-label="Id"><%= package.package_id %></div> -->
          <div class="col col-2" data-label="Source"><%= package.s_name %></div>
          <div class="col col-3" data-label="Destination"><%= package.d_name %></div>
          <div class="col col-4" data-label="Duration"><%= package.duration %></div>
          <div class="col col-5" data-label="Price"><%= package.price %></div>
          <div class="col col-6" data-label="Rating"><%= package.rating %></div>
        </li>
      <% }) %>
    </ul>
  </div>